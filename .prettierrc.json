{
	"printWidth": 120,
	"singleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"tabWidth": 4,
	"semi": false,
	"useTabs": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"importOrder": [
		"^@domain/(.*)$",
		"^@services/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}